        <method name="priv_i_cal_vcardstructuredtype_clone" corresponds="none" kind="private" since="4.0" annotation="skip">
                <parameter type="const vcardstructuredtype *" name="src" annotation="transfer none" comment="vcardstructuredtype to clone"/>
                <returns type="vcardstructuredtype *" annotation="transfer full" comment="The newly created vcardstructuredtype"/>
                <custom>    guint ii;
    vcardstructuredtype *res;
    if (src == NULL)
        return NULL;
    res = vcardstructured_new();
    res->num_fields = src->num_fields;
    for (ii = 0; ii &lt; src->num_fields; ii++) {
        vcardstrarray *field = src->field[ii];
        res->field[ii] = field ? vcardstrarray_clone(field) : NULL;
    }
    return res;</custom>
        </method>
	<method name="i_cal_vcard_value_new_structured" corresponds="vcardvalue_new_structured" kind="constructor" since="4.0">
		<parameter type="const ICalVcardStructured *" name="v" annotation="transfer none" comment="The value"/>
		<returns type="ICalVcardValue *" annotation="transfer full" comment="The newly created #ICalVcardValue." />
		<comment xml:space="preserve">Note: this uses a copy of the structured type, not the one stored in the @v</comment>
		<custom>    vcardstructuredtype *val;
    g_return_val_if_fail(v != NULL, NULL);
    val = (vcardstructuredtype *)i_cal_object_get_native ((ICalObject *)v);
    if (val == NULL)
        return NULL;
    return i_cal_vcard_value_new_full(vcardvalue_new_structured(priv_i_cal_vcardstructuredtype_clone(val)), NULL);</custom>
	</method>
