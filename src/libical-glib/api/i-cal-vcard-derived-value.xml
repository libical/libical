        <method name="priv_i_cal_vcardstructuredtype_clone" corresponds="none" kind="private" since="4.0" annotation="skip">
                <parameter type="const vcardstructuredtype *" name="src" annotation="transfer none" comment="vcardstructuredtype to clone"/>
                <returns type="vcardstructuredtype *" annotation="transfer full" comment="The newly created vcardstructuredtype"/>
                <custom>    guint ii;
    vcardstructuredtype *res;
    if (src == NULL)
        return NULL;
    res = vcardstructured_new();
    res->num_fields = src->num_fields;
    for (ii = 0; ii &lt; src->num_fields; ii++) {
        vcardstrarray *field = src->field[ii];
        res->field[ii] = field ? vcardstrarray_clone(field) : NULL;
    }
    return res;</custom>
        </method>
