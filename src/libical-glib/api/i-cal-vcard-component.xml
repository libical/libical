    <parameter type="ICalVcardProperty *" name="property" annotation="transfer full" comment="An #ICalVcardProperty"/>
    <comment xml:space="preserve">Adds the @property into the @component and unrefs the @property.</comment>
    <custom>    g_return_if_fail(I_CAL_VCARD_IS_COMPONENT(component));
    g_return_if_fail(I_CAL_IS_PROPERTY(property));

