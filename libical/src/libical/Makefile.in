ical.h: $(COMBINEDHEADERS)
	echo '#ifdef __cplusplus'         >  ical.h
	echo 'extern "C" {'               >> ical.h
	echo '#endif'                     >> ical.h
	echo '/*'                         >> ical.h
	echo ' $$''Id''$$'                >> ical.h
	echo '*/'                         >> ical.h
	cat $(COMBINEDHEADERS)				\
	| egrep -v "#include.*\"ical"			\
	| egrep -v "#include.*\"config"			\
	| egrep -v "#include.*\"pvl\.h\""		\
	| egrep -v '\$$(Id|Locker): .+\$$'>> ical.h
	echo '#ifdef __cplusplus'         >> ical.h
	echo '};'                         >> ical.h
	echo '#endif'                     >> ical.h

