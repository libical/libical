[library]
version = "@CMAKE_PROJECT_VERSION_MAJOR@"
browse_url = "https://github.com/libical/libical"
repository_url = "https://github.com/libical/libical.git"
website_url = "https://libical.github.io/libical/"
docs_url = "https://libical.github.io/libical/libical-glib/"
authors = "libical Development Team"
license = "LGPL-2.1-only OR MPL-2.0"
description = "iCalendar protocols and protocol data units implementation"
dependencies = ["GObject-2.0", "Gio-2.0"]
devhelp = true
search_index = true

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/gobject/"

  [dependencies."Gio-2.0"]
  name = "Gio"
  description = "GObject Interfaces and Objects, Networking, IPC, and I/O"
  docs_url = "https://docs.gtk.org/gio/"

[theme]
name = "basic"
show_index_summary = true
show_class_hierarchy = true

[source-location]
base_url = "https://github.com/libical/libical/blob/master/"

[extra]
urlmap_file = "urlmap.js"
